using System;

namespace nine.one
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Введите номер дня ");
         
            int day = Convert.ToInt32(Console.ReadLine());
            int caseSwitch = day;
            switch (caseSwitch)
                {
                case 1:
            Console.WriteLine("первое");
                break;
            case 2:
            Console.WriteLine("второе");
                break;
                Console.WriteLine(day);
                case 3:
                    Console.WriteLine("третье");
                    break;
                case 4:
                    Console.WriteLine("четвертое");
                    break;
                    Console.WriteLine(day);
                case 5:
                    Console.WriteLine("пятое");
                    break;
                case 6:
                    Console.WriteLine("шестое");
                    break;
                    Console.WriteLine(day);
                case 7:
                    Console.WriteLine("седьмое");
                    break;
                case 8:
                    Console.WriteLine("восьмое");
                    break;
                    Console.WriteLine(day);
                case 9:
                    Console.WriteLine("девятое");
                    break;
                case 10:
                    Console.WriteLine("десятое");
                    break;
                    Console.WriteLine(day);
                case 11:
                    Console.WriteLine("одиннадцатое");
                    break;
                case 12:
                    Console.WriteLine("двенадцатое");
                    break;
                    Console.WriteLine(day);
                case 13:
                    Console.WriteLine("тринадцатое");
                    break;
                case 14:
                    Console.WriteLine("четырнадцатое");
                    break;
                    Console.WriteLine(day);
                case 15:
                    Console.WriteLine("пятнадцатое");
                    break;
                case 16:
                    Console.WriteLine("шестнадцатое");
                    break;
                    Console.WriteLine(day);
                case 17:
                    Console.WriteLine("семнадцатое");
                    break;
                case 18:
                    Console.WriteLine("восемнадцатое");
                    break;
                    Console.WriteLine(day);
                case 19:
                    Console.WriteLine("девятнадцатое");
                    break;
                case 20:
                    Console.WriteLine("двадцатое");
                    break;
                    Console.WriteLine(day);
                case 21:
                    Console.WriteLine("двадцать первое");
                    break;
                    Console.WriteLine(day);
                case 22:
                    Console.WriteLine("двадцать второе");
                    break;
                case 23:
                    Console.WriteLine("двадцать третье");
                    break;
                    Console.WriteLine(day);
                case 24:
                    Console.WriteLine("двадцать четвертое");
                    break;
                case 25:
                    Console.WriteLine("двадцать пятое");
                    break;
                    Console.WriteLine(day);
                case 26:
                    Console.WriteLine("двадцать шестое");
                    break;
                    Console.WriteLine(day);
                case 27:
                    Console.WriteLine("двадцать седьмое");
                    break;
                    Console.WriteLine(day);
                case 28:
                    Console.WriteLine("двадцать восьмое");
                    break;
                case 29:
                    Console.WriteLine("двадцать девятое");
                    break;
                    Console.WriteLine(day);
                case 30:
                    Console.WriteLine("тридцатое");
                    break;
                case 31:
                    Console.WriteLine("тридцать первое");
                    break;
                    Console.WriteLine(day);
                default:
                    Console.WriteLine("нет такого числа в месяце как бэ");
                    break;
            }
            Console.WriteLine("Введите номер месяца ");
            int mon = Convert.ToInt32(Console.ReadLine());
            int caseSwitch2 = mon;
            switch (mon)
            {
                case 1:
                    Console.WriteLine(" января");
                    break;
                case 2:
                    Console.WriteLine(" февраля");
                    break;
                case 3:
                    Console.WriteLine(" марта");
                    break;
                case 4:
                    Console.WriteLine(" апреля");
                    break;
                case 5:
                    Console.WriteLine(" мая");
                    break;
                case 6:
                    Console.WriteLine(" июня");
                    break;
                case 7:
                    Console.WriteLine(" июля");
                    break;
                case 8:
                    Console.WriteLine(" августа");
                    break;
                case 9:
                    Console.WriteLine(" сентября");
                    break;
                case 10:
                    Console.WriteLine(" октября");
                    break;
                case 11:
                    Console.WriteLine(" ноября");
                    break;
                case 12:
                    Console.WriteLine(" декабря");
                    break;
                default:

                    Console.WriteLine("месяцев 12, а не 13, 14 и т.д.");
                    break;
            }
        }
    }
}
using System;

namespace nine.two
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Введите номер эл-та, если 0 - С, 1 - В, 2 - Ю, 3 - З ");
        
            int[] array = new int[4] { 0, 1, 2, 3 };
            int i = Convert.ToInt32(Console.ReadLine());
            Console.WriteLine("Введите команду, где 0 - продолжить движение, -1 - повернуть направо, 1 - повернуть налево ");
            int N = Convert.ToInt32(Console.ReadLine());
            if (i == 0)
            {
 switch (N)
            {
                case 0:

                    Console.WriteLine("Ваше направление cеверное");
                    break;
                case 1:
                    Console.WriteLine("Ваше направление восточное");
                    break;
                case -1:
                    Console.WriteLine("Ваше напрвадение западное");
                    break;
              
                default:
                    Console.WriteLine("СЛЕДУЙТЕ ИНСТРУКЦИЯМ!");
                    break;

            }

            }

            if (i == 1)
            {
                switch (N)
                {
                    case 0:

                        Console.WriteLine("Ваше направление восточное");
                        break;
                    case 1:
                        Console.WriteLine("Ваше направление северное");
                        break;
                    case -1:
                        Console.WriteLine("Ваше напрвадение южное");
                        break;

                    default:
                        Console.WriteLine("СЛЕДУЙТЕ ИНСТРУКЦИЯМ!");
                        break;

                }

            }

            if (i == 2)
            {
                switch (N)
                {
                    case 0:

                        Console.WriteLine("Ваше направление южное");
                        break;
                    case 1:
                        Console.WriteLine("Ваше направление восточное");
                        break;
                    case -1:
                        Console.WriteLine("Ваше напрвадение западное");
                        break;

                    default:
                        Console.WriteLine("СЛЕДУЙТЕ ИНСТРУКЦИЯМ!");
                        break;

                }

            }
            if (i == 3)
            {
                switch (N)
                {
                    case 0:

                        Console.WriteLine("Ваше направление западное");
                        break;
                    case 1:
                        Console.WriteLine("Ваше направление южное");
                        break;
                    case -1:
                        Console.WriteLine("Ваше напрвадение северное");
                        break;

                    default:
                        Console.WriteLine("СЛЕДУЙТЕ ИНСТРУКЦИЯМ!");
                        break;

                }

            }

        }
    }
}
using System;

namespace nine.three
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Введите кол-во номеров заданий от 10 до 40");
            int N = Convert.ToInt32(Console.ReadLine());
          
          switch (N)
            {
                case 10:
                    Console.WriteLine("десять учебных заданий");
                    break;
                case 11:
                    Console.WriteLine("одиннадцать учебных заданий");
                    break;
                case 12:
                    Console.WriteLine("двенадцать учебных заданий");
                    break;
                case 13:
                    Console.WriteLine("тринадцать учебных заданий");
                    break;
                case 14:
                    Console.WriteLine("четырнадцать учебных заданий");
                    break;
                case 15:
                    Console.WriteLine("пятнадцать учебных заданий");
                    break;
                case 16:
                    Console.WriteLine("шестнадцать учебных заданий");
                    break;
                case 17:
                    Console.WriteLine("семнадцать учебных заданий");
                    break;
                case 18:
                    Console.WriteLine("восемнадцать учебных заданий");
                    break;
                case 19:
                    Console.WriteLine("девятнадать учебных заданий");
                    break;
                case 20:
                    Console.WriteLine("двадцать учебных заданий");
                    break;
                case 21:
                    Console.WriteLine("двадцать одно учебное задание");
                    break;
                case 22:
                    Console.WriteLine("двадцать два учебных задания");
                    break;
                case 23:
                    Console.WriteLine("двадцать три учебных задания");
                    break;
                case 24:
                    Console.WriteLine("двадцать четыре учебных задания");
                    break;
                case 25:
                    Console.WriteLine("двадцать пять учебных заданий");
                    break;
                case 26:
                    Console.WriteLine("двадцать шесть учебных заданий");
                    break;
                case 27:
                    Console.WriteLine("двадцать семь учебных заданий");
                    break;
                case 28:
                    Console.WriteLine("двадцать восемь учебных заданий");
                    break;
                case 29:
                    Console.WriteLine("двадцать девять учебных заданий");
                    break;
                case 30:
                    Console.WriteLine("тридцать учебных заданий");
                    break;
                case 31:
                    Console.WriteLine("тридцать одно учебное задание");
                    break;
                case 32:
                    Console.WriteLine("тридцать два учебных задания");
                    break;
                case 33:
                    Console.WriteLine("тридцать три учебных задания");
                    break;
                case 34:
                    Console.WriteLine("тридцать четыре учебных задания");
                    break;
                case 35:
                    Console.WriteLine("тридцать пять учебных заданий");
                    break;
                case 36:
                    Console.WriteLine("тридцать шесть учебных заданий");
                    break;
                case 37:
                    Console.WriteLine("тридцать семь учебных заданий");
                    break;
                case 38:
                    Console.WriteLine("тридцать восемь учебных заданий");
                    break;
                case 39:
                    Console.WriteLine("тридцать девять учебных заданий");
                    break;
                case 40:
                    Console.WriteLine("сорок учебных заданий");
                    break;
                default:
                    Console.WriteLine("а стока нету");
                    break;
            }
        }
    }
}
using System;

namespace nine.four
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Введите число");
            int n = Convert.ToInt32(Console.ReadLine());
            switch (n / 100)
            {
                case 1:
                    Console.WriteLine("сто");
                   
                    break;
                case 2:
                    Console.WriteLine("двести");
                    break;
                case 3:
                    Console.WriteLine("триста");
                    break;
                case 4:
                    Console.WriteLine("четыреста");
                    break;
                case 5:
                    Console.WriteLine("пятьсот");
                    break;
                case 6:
                    Console.WriteLine("шестьсот");
                    break;
                case 7:
                    Console.WriteLine("семьсот");
                    break;
                case 8:
                    Console.WriteLine("восемьсот");
                    break;
                case 9:
                    Console.WriteLine("девятьсот");
                    break;
            }
            switch (n % 100 - n % 10)
            {
                case 20:
                    Console.WriteLine("двадцать");
                    break;
                case 30:
                    Console.WriteLine("тридцать");
                    break;
                case 40:
                    Console.WriteLine("сорок");
                    break;
                case 50:
                    Console.WriteLine("пятьдесят");
                    break;
                case 60:
                    Console.WriteLine("шестьдесят");
                    break;
                case 70:
                    Console.WriteLine("семьдесят");
                    break;
                case 80:
                    Console.WriteLine("восемьдесят");
                    break;
                case 90:
                    Console.WriteLine("девяносто");
                    break;

            }
            switch (n%100)
            {
                case 11:
                    Console.WriteLine("одиннадцать");
                    break;
                case 12:
                    Console.WriteLine("двенадцать");
                    break;
                case 13:
                    Console.WriteLine("тринадцать");
                    break;
                case 14:
                    Console.WriteLine("четырнадцать");
                    break;
                case 15:
                    Console.WriteLine("пятнадцать");
                    break;
                case 16:
                    Console.WriteLine("шестнадцать");
                    break;
                case 17:
                    Console.WriteLine("семнадцать");
                    break;
                case 18:
                    Console.WriteLine("восемнадцать");
                    break;
                case 19:
                    Console.WriteLine("девятнадцать");
                    break;
            }
            switch (n%10)
            {
                case 1:
                    Console.WriteLine("один");
                    break;
                case 2:
                    Console.WriteLine("два");
                    break;
                case 3:
                    Console.WriteLine("три");
                    break;
                case 4:
                    Console.WriteLine("четыре");
                    break;
                case 5:
                    Console.WriteLine("пять");
                    break;
                case 6:
                    Console.WriteLine("шесть");
                    break;
                case 7:
                    Console.WriteLine("семь");
                    break;
                case 8:
                    Console.WriteLine("восемь");
                    break;
                case 9:
                    Console.WriteLine("девять");
                    break;
            }
        }
    }
}
